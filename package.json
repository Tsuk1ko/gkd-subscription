{
  "name": "@tsuk1ko/gkd-subscription",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/gkd.json5",
  "scripts": {
    "postinstall": "simple-git-hooks",
    "format": "prettier --cache --write ./**/*.{mjs,ts}",
    "lint": "eslint --cache --fix ./**/*.{mjs,ts}",
    "check": "tsc --noEmit && tsx ./scripts/check.ts",
    "build": "tsc --noEmit && tsx ./scripts/build.ts"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "pre-push": "pnpm run check"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,jsx,tsx}": [
      "eslint --cache --fix",
      "prettier --cache --write"
    ],
    "*.json": [
      "prettier --cache --write"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@eslint/js": "9.21.0",
    "@gkd-kit/api": "0.6.0",
    "@gkd-kit/define": "0.0.1",
    "@gkd-kit/tools": "0.6.0",
    "@types/node": "22.13.9",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.2",
    "eslint-plugin-unused-imports": "4.1.4",
    "json5": "2.2.3",
    "lint-staged": "15.4.3",
    "prettier": "3.5.3",
    "simple-git-hooks": "2.11.1",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.0"
  },
  "packageManager": "pnpm@10.5.2",
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "simple-git-hooks"
    ]
  }
}
